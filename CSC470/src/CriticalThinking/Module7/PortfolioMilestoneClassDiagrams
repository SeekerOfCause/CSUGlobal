@startuml

abstract class Vehicle {
  +vehicleID: int
  +color: String
}
class Car {
  +make: String
  +model: String
}
class Truck {
  +payloadWeightLimitLbs: int
  +truckBedSizeSqFt: int
}
Car --|> Vehicle
Truck --|> Vehicle

class Driver {
  +driverID : int
  +name : String
}
class Car {
  +vehicleID : int
  +color : String
  +make : String
  +model : String
}
Driver "1" -- "N" Car

@enduml

