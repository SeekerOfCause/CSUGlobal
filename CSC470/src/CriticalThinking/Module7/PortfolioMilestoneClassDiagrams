@startuml

abstract class Vehicle<<table>> {
  +vehicleID: int
  +color: String
}
class Car<<table>> {
  +vehicleID : int
  +color : String
  +make: String
  +model: String
}
class Truck<<table>> {
  +vehicleID : int
  +color : String
  +payloadWeightLimitLbs: int
  +truckBedSizeSqFt: int
}
Car --|> Vehicle
Truck --|> Vehicle


@enduml

